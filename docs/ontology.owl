    <owl:Class rdf:about="https://marcospiedra.github.io/bcfy/ontology.owl#City">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://marcospiedra.github.io/bcfy/ontology.owl#hasCoordinate"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://marcospiedra.github.io/bcfy/ontology.owl#Coordinate"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment>City, mainly for activites</rdfs:comment>
        <rdfs:label>City</rdfs:label>
    </owl:Class>
    


