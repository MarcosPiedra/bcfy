    <!-- https://marcospiedra.github.io/bcfy/ontology.owl#Activities -->


    <owl:Class rdf:about="https://marcospiedra.github.io/bcfy/ontology.owl#Activities">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://marcospiedra.github.io/bcfy/ontology.owl#hasCity"/>
                <owl:someValuesFrom rdf:resource="https://marcospiedra.github.io/bcfy/ontology.owl#City"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment>Activities in the city</rdfs:comment>
        <rdfs:label>Activities</rdfs:label>
    </owl:Class>
    


